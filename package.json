    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "babel-jest": "^27.3.1",
    "babel-plugin-styled-components": "^1.13.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
